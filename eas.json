{
  "cli": {
    "version": ">= 15.0.9",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "resourceClass": "large"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "version": "0.1.0",
      "android": {
        "buildType": "app-bundle",
        "versionCode": 1
      },
      "ios": {
        "buildNumber": "2" // Tambahkan buildNumber untuk iOS
      }
    },
    "submit": {
      "production": {}
    }
  }